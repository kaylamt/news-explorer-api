{
  "extends": "airbnb-base",
  "overrides": [
    {
      "files": [
        "*.js"
      ],
      "rules": {
        "no-underscore-dangle": [
          "error",
          {
            "allow": [
              "_id"
            ]
          }
        ],
        "no-unused-vars": [
          "error",
          {
            "argsIgnorePattern": "next"
          }
        ]
      }
    }
  ]
}